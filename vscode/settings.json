{
  // better-comments ---
  "better-comments.highlightPlainText": true,
  // breadcrumbs ---
  "breadcrumbs.enabled": true,
  // debug ---
  "debug.javascript.codelens.npmScripts": "never",
  // diffEditor ---
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.renderSideBySide": true,
  // editor --
  // "editor.fontFamily": "Menlo, Source Code Pro, Dank Mono, Monaco, 'Courier New', monospace",
  // "editor.fontFamily": "Source Code Pro, Dank Mono, Monaco, 'Courier New', monospace",
  "editor.fontFamily": "Dank Mono, Source Code Pro, Monaco, 'Courier New', monospace",
  // "editor.fontFamily": "JetBrains Mono, Dank Mono,Source Code Pro, Monaco, 'Courier New', monospace",
  "editor.fontSize": 14,
  "editor.fontWeight": "400",
  "editor.fontLigatures": true,
  "editor.cursorWidth": 3,
  // "editor.cursorStyle": "block",
  "editor.tabSize": 2,
  "editor.hover.delay": 300,
  "editor.quickSuggestionsDelay": 300,
  "editor.trimAutoWhitespace": true,
  "editor.cursorSmoothCaretAnimation": true,
  "editor.suggestSelection": "first",
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "editor.rulers": [80],
  "editor.formatOnSave": true,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.maxColumn": 200,
  "editor.minimap.showSlider": "always",
  "editor.smoothScrolling": true,
  "editor.renderWhitespace": "none",
  "editor.accessibilityPageSize": 14,
  "editor.columnSelection": false,
  "editor.maxTokenizationLineLength": 10000,
  "editor.parameterHints.cycle": true,
  "editor.tabCompletion": "on",
  "editor.cursorBlinking": "phase",
  "editor.find.globalFindClipboard": true,
  "editor.quickSuggestions": {
    "comments": true,
    "other": true,
    "strings": true
  },
  // emmet ---
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  // files ---
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true,
    "**/**.class": true
  },
  // git ---
  "git.enableSmartCommit": true,
  // search ---
  "search.exclude": {
    "**/.next/": true,
    "**/coverage": true
  },
  // telemetry --
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  // terminal ---
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.shell.osx": "/bin/zsh",
  "terminal.integrated.fontFamily": "Dank Mono, Monaco, 'Courier New', monospace",
  // window --
  "window.zoomLevel": 1,
  "window.title": "${dirty} ${activeEditorMedium}${separator}${rootName}",
  "workbench.settings.editor": "json",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.preferredDarkColorTheme": "Visual Studio Dark",
  "workbench.preferredHighContrastColorTheme": "Visual Studio Dark",
  "workbench.sideBar.location": "left",
  "editor.minimap.enabled": false,
  "workbench.statusBar.visible": true,
  "workbench.activityBar.visible": true,
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.statusBar.feedback.visible": false,
